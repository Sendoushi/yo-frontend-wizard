{
  "name": "<%= name %>",
  "version": "<%= version %>",
  "devDependencies": {<% if (!!tech.babel) { %>
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.29",<% } %><% if (structure === 'html') { %>
    "grunt-html-build": "^0.5.2",<% } %><% if (!!tech.sass) { %>
    "grunt-sass": "^1.0.0",
    "grunt-sass-globbing": "^1.4.0",
    "node-sass": "^3.3.3",
    "sass": "^0.5.0",<% } %>
    "grunt-autoprefixer": "^3.0.3",
    "grunt-pixrem": "^0.1.2",
    "grunt-svgmin": "^3.1.0",
    "express": "^4.13.3",
    "grunt": "^0.4.5",
    "grunt-contrib-copy": "^0.7.0",
    "grunt-contrib-cssmin": "^0.12.1",<% if (structure !== 'php') { %>
    "grunt-contrib-htmlmin": "^0.6.0",<% } else { %>
    "grunt-php": "^1.5.1",<% } %>
    "grunt-contrib-uglify": "^0.7.0",
    "grunt-webpack": "^1.0.11",
    "json-loader": "^0.5.4",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.4.4",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.14.0"
  },<% if (!tech.bower) { %>
  "dependencies": {<% if (!!tech.jquery) { %>
    "jquery": "~2.1.4",<% } %><% if (!!tech.promises) { %>
    "bluebird": "~3.1.1",<% } %><% if (!!tech.foundation) { %>
    "foundation-sites": "^6.0.3",<% } %><% if (structure === "backbone_bedrock") { %>
    "Bedrock": "git+ssh://git@github.com/Sendoushi/bedrock-web.git",<% } else if (structure === "redux_riot") { %>
    "redux": "^3.0.4",
    "redux-thunk": "^1.0.0",
    "riot": "^2.3.1",<% } %>
    "mout": "~0.11.1",
    "outdated-browser": "~1.0.2"
  },<% } %>
  "scripts": {
    "test": "# TODO",
    "build": "node -e \"require('grunt').tasks(['build'], { gruntfile: require('path').join(process.cwd(), 'tasks/build.js') })\"",
    "server": "node -e \"require('grunt').tasks(['server'], { gruntfile: require('path').join(process.cwd(), 'tasks/server.js') })\"",
    "start": "npm run server"
  }
}
